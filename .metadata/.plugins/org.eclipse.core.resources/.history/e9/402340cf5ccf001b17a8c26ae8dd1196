
public class T2 {

	public static void main(String[] args) {
		String str="132656";
		int fin=0;
		int num = 0;
		for(int i=0;i<str.length();i++){
			char x=str.charAt(i);
			num=(int)x;
			num=num-48;
			//System.out.println(num);
			if(i>0){
				int xyz = 0;
				for(int j=0;j<(str.length()-i);i++){
					xyz=xyz*10;
				}
				fin=fin+(num*xyz);
			}
			else{
				fin=fin+num;
			}
			System.out.println(fin);
		}
		
	}

}
